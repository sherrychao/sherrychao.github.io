<script type="text/javascript">
      function issue1yay(e){
        e.preventDefault();
        var form = e.target;
        var email = form["email"].value;

        var user = {
            email: email, 
          
        };
        analytics.identify(email, {
            email: email, 
            
        });
        analytics.track('voted', {
           email: email,
           choice:'yay',
           billId:'H123',
           categories:['business','economy', 'taxes']
        });
        window.location.href = "";
      }
</script>

